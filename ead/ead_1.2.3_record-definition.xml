<?xml version="1.0"?>
<record-definition prefix="ead" version="1.2.3" flat="false" elementFormDefaultQualified="true"
                   attributeFormDefaultQualified="false">

    <namespaces>
        <namespace prefix="ead" uri="urn:isbn:1-931666-22-9" schema="http://schemas.archivesportaleurope.net/profiles/apeEAD.xsd"/>
        <namespace prefix="xlink" uri="http://www.w3.org/1999/xlink" schema="http://www.loc.gov/standards/xlink/xlink.xsd"/>
    </namespaces>

    <attrs>
        <attr tag="abbr"/>
        <attr tag="audience"/>
        <attr tag="authfilenumber"/>
        <attr tag="calendar"/>
        <attr tag="cols"/>
        <attr tag="countrycode"/>
        <attr tag="countryencoding"/>
        <attr tag="dateencoding"/>
        <attr tag="encodinganalog"/>
        <attr tag="era"/>
        <attr tag="expan"/>
        <attr tag="id"/>
        <attr tag="identifier"/>
        <attr tag="label"/>
        <attr tag="langcode"/>
        <attr tag="langencoding"/>
        <attr tag="level"/>
        <attr tag="mainagencycode"/>
        <attr tag="normal"/>
        <attr tag="numeration"/>
        <attr tag="render"/>
        <attr tag="relatedencoding"/>
        <attr tag="repositoryencoding"/>
        <attr tag="scriptcode"/>
        <attr tag="scriptencoding"/>
        <attr tag="type"/>
        <attr tag="url"/>
        <attr tag="unit"/>
        <attr tag="xlink:href"/>
        <attr tag="xlink:title"/>
    </attrs>

    <elems>
        <elem tag="abbr" attrs="expan"/>
        <elem tag="address" elems="addressline"/>
        <elem tag="addressline"/>
        <elem tag="bibref" attrs="xlink:href" elems="name, title, imprint"/>
        <elem tag="corpname" attrs="authfilenumber"/>
        <!--todo <elem tag="emph" attrs="render"/>-->
        <elem tag="dao" attrs="xlink:href, xlink:title"/>
        <elem tag="date" attrs="era, calendar, normal, encodinganalog"/>
        <elem tag="dimensions" attrs="type, unit"/>
        <elem tag="descrules" attrs="encodinganalog"/>
        <elem tag="entry"/>
        <elem tag="expan" attrs="abbr"/>
        <elem tag="extent" attrs="unit"/>
        <elem tag="extref" attrs="xlink:href, xlink:title"/>
        <elem tag="famname" attrs="authfilenumber"/>
        <elem tag="function"/>
        <elem tag="genreform"/>
        <elem tag="geogname"/>
        <elem tag="head"/>
        <elem tag="imprint" elems="publisher, geogname, date"/>
        <elem tag="item"/>
        <elem tag="language" attrs="langcode, scriptcode, encodinganalog"/>
        <elem tag="lb"/>
        <elem tag="list" elems="head, item" attrs="type, numeration"/>
        <elem tag="occupation"/>
        <elem tag="name" attrs="authfilenumber"/>
        <elem tag="p"/>
        <!--todo <elem tag="p" elems="lb, emph, abbr, expan, extref"/>-->
        <elem tag="persname" attrs="authfilenumber"/>
        <elem tag="physfacet" attrs="type, type"/>
        <elem tag="publisher" attrs="encodinganalog"/>
        <elem tag="row" elems="entry"/>
        <elem tag="subject"/>
        <elem tag="table" elems="head, tgroup"/>
        <elem tag="tgroup" elems="thead, tbody" attrs="cols"/>
        <elem tag="thead" elems="row"/>
        <elem tag="tbody" elems="row"/>
        <elem tag="title"/>
        <elem tag="titleproper" attrs="type, encodinganalog"/>
        <!--todo <elem tag="titleproper" elems="lb, emph" attrs="type, encodinganalog"/>-->

        <!-- did -->
        <elem tag="did">
            <elem tag="container" attrs="type"/>
            <elem tag="dao" attrs="xlink:href, xlink:title"/>
            <elem tag="langmaterial" attrs="encodinganalog" elems="language"/>
            <elem tag="materialspec"/>
            <elem tag="note" elems="p" attrs="encodinganalog, label"/>
            <elem tag="origination" elems="corpname, famname, persname, name" attrs="label, encodinganalog"/>
            <elem tag="physdesc" elems="genreform, dimensions, physfacet, extent" attrs="encodinganalog"/>
            <elem tag="physloc" attrs="label"/>
            <elem tag="repository" elems="address"/>
            <elem tag="unitdate" attrs="calendar, era, normal, encodinganalog"/>
            <elem tag="unitid" attrs="encodinganalog, type"/>
            <elem tag="unittitle" attrs="encodinganalog"/>
        </elem>
    </elems>


    <elem-groups>

        <elem-group name="m.desc.base.lowlevels">

            <elem tag="accessrestrict" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="accruals" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="altformavail" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="appraisal" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="bioghist" elems="head, p, list, dao, table" attrs="encodinganalog"/>
            <elem tag="controlaccess" elems="head, p, corpname, geogname, famname, name, occupation, persname, subject, function, title"/>
            <elem tag="custodhist" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="odd" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="otherfindaid" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="phystech" elems="head, p" attrs="encodinganalog"/>
            <elem tag="processinfo" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="scopecontent" elems="head, list, table, p, table" attrs="encodinganalog"/>
            <elem tag="userestrict" elems="head, list, table, p" attrs="encodinganalog"/>

        </elem-group>


        <elem-group name="m.desc.base.highlevels">

            <elem tag="acqinfo" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="arrangement" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="bibliography" elems="head, p, bibref" attrs="encodinganalog"/>
            <elem tag="fileplan" elems="head, list, table, p"/> <!-- THE ONLY BIG P -->
            <elem tag="originalsloc" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="prefercite" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="relatedmaterial" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="separatedmaterial" elems="head, list, table, p" attrs="encodinganalog"/>

        </elem-group>

        <elem-group name="c_group">

            <elem tag="acqinfo" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="arrangement" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="bibliography" attrs="encodinganalog">
                <elem tag="head"/>
                <elem tag="p"/>
                <elem tag="bibref" attrs="xlink:href" elems="name, title, imprint"/>
            </elem>
            <elem tag="fileplan" elems="head, list, table, p"/> <!-- THE ONLY BIG P -->
            <elem tag="originalsloc" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="prefercite" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="relatedmaterial" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="separatedmaterial" elems="head, list, table, p" attrs="encodinganalog"/>


            <elem tag="accessrestrict" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="accruals" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="altformavail" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="appraisal" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="bioghist" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="controlaccess" elems="head">
                <elem tag="p"/>
                <!--todo <elem tag="p" elems="lb, emph, extref"/>-->
                <elem tag="name"/>
                <elem tag="geogname"/>
                <elem tag="famname"/>
                <elem tag="subject"/>
                <elem tag="corpname"/>
                <elem tag="persname"/>
                <elem tag="occupation"/>
                <elem tag="function"/>
                <elem tag="title"/>
                <elem tag="genreform"/>
            </elem>
            <elem tag="custodhist" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="odd" elems="head, p, list, table" attrs="encodinganalog"/>
            <elem tag="otherfindaid" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="phystech" elems="head, p" attrs="encodinganalog"/>
            <elem tag="processinfo" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="scopecontent" elems="head, list, table, p" attrs="encodinganalog"/>
            <elem tag="userestrict" elems="head, list, table, p" attrs="encodinganalog"/>

        </elem-group>
    </elem-groups>


    <root tag="ead" attrs="audience">

        <elem tag="eadheader" attrs="langencoding, scriptencoding, dateencoding, countryencoding, repositoryencoding, relatedencoding">
            <elem tag="eadid" attrs="url, countrycode, mainagencycode, identifier  "/>
            <elem tag="filedesc">
                <elem tag="titlestmt" elems="titleproper">
                    <elem tag="subtitle"/>
                    <!--todo <elem tag="subtitle" elems="lb, emph"/>-->
                    <elem tag="author" attrs="encodinganalog"/>
                </elem>
                <elem tag="publicationstmt" elems="publisher, date, address"/>
                <elem tag="seriesstmt" elems="titleproper">
                </elem>
            </elem>
            <elem tag="profiledesc">
                <elem tag="creation" elems="date"/>
                <elem tag="langusage" elems="language"/>
                <elem tag="descrules"/>
            </elem>
            <elem tag="revisiondesc" attrs="encodinganalog">
                <!--<elem tag="list" elems="head, item"/>  &lt;!&ndash; ITEM contatins LIST acc to XSD CHECK AGAIN&ndash;&gt;-->
                <elem tag="change" elems="date, item"/>

            </elem>
        </elem>

        <!-- Archival description -->
        <elem tag="archdesc" elems="did" elem-groups="m.desc.base.lowlevels, m.desc.base.highlevels" attrs="level, type, encodinganalog, relatedencoding">

            <!-- Detailed descriptions -->
            <elem tag="dsc" elems="head, list, table, p" attrs="type">

                <elem tag="c" elems="did" elem-groups="c_group" attrs="level, encodinganalog, id">
                    <elem tag="c" elems="did" elem-groups="c_group" attrs="level, encodinganalog, id">
                        <elem tag="c" elems="did" elem-groups="c_group" attrs="level, encodinganalog, id">
                            <elem tag="c" elems="did" elem-groups="c_group" attrs="level, encodinganalog, id">
                                <elem tag="c" elems="did" elem-groups="c_group" attrs="level, encodinganalog, id">
                                    <elem tag="c" elems="did" elem-groups="c_group" attrs="level, encodinganalog, id">
                                        <elem tag="c" elems="did" elem-groups="c_group" attrs="level, encodinganalog, id">
                                            <elem tag="c" elems="did" elem-groups="c_group" attrs="level, encodinganalog, id">
                                                <elem tag="c" elems="did" elem-groups="c_group" attrs="level, encodinganalog, id">
                                                </elem>
                                            </elem>
                                        </elem>
                                    </elem>
                                </elem>
                            </elem>
                        </elem>
                    </elem>
                </elem>
            </elem>
        </elem>

    </root>


    <field-markers>
        <!--<field-marker name="dataProvider" type="fact" path="/record/admin/displayDataProvider"/>-->
        <!--<field-marker name="country" type="fact" path="/record/admin/displayDataProviderCountry"/>-->
        <!--<field-marker name="provider" type="fact" path="/record/admin/displayProvider"/>-->

    </field-markers>

