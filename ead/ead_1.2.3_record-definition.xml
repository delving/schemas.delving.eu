                        <!--<dict path="/record/facets/facet" key="@key" value="@type" name="Facets">-->
                            <!--<entry value="A"/>-->
                            <!--<entry value="B"/>-->
                        <!--</dict>-->
                    <!--</dicts>-->
                <!--</opt>-->
            <!--</opt-list>-->
    <!--</opts>-->


</record-definition>
