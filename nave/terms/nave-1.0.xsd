                           <element ref="nave:dimension" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:event" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:exhibition" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:formatted" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:location" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:material" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:objectNumber" minOccurs="1" maxOccurs="1"/>
                           <element ref="nave:objectSoort" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:pageEnd" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:pageStart" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:pages" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:person" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:place" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:productionEnd" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:productionPeriod" minOccurs="0" maxOccurs="unbounded"/>
