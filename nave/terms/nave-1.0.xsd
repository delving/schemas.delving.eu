                            <element ref="technique" minOccurs="0" maxOccurs="unbounded"/>
                        </choice>
                    </sequence>
                </extension>
            </complexContent>
        </complexType>
    </xs:element>

    <xs:element name="Museometrie">
        <complexType>
            <annotation>
                <documentation>ResourceType contains Museometrie elements.</documentation>
            </annotation>
            <complexContent>
