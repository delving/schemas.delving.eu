                            <element ref="videoUri" minOccurs="0" maxOccurs="unbounded"/>
                        </choice>
                    </sequence>
                </extension>
            </complexContent>
        </complexType>
    </xs:element>

    <xs:element name="RijksCollection">
        <complexType>
            <annotation>
                <documentation>ResourceType contains RijksCollection elements.</documentation>
            </annotation>
            <complexContent>
                <extension base="rdf:AboutType">
                    <sequence minOccurs="0" maxOccurs="unbounded">
                        <element ref="ore:isAggregatedBy" minOccurs="1" maxOccurs="1"/>
                        <choice>
                            <element ref="acceptedStateCharges" minOccurs="0" maxOccurs="1"/>
                            <element ref="acceptedStateChargesReason" minOccurs="0" maxOccurs="unbounded"/>
                            <element ref="expulsionMeans" minOccurs="0" maxOccurs="1"/>
