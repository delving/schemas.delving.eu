    <xs:element name="BrabantCloudResource">
        <complexType name="BrabantCloudType">
            <annotation>
                <documentation>ResourceType that contain the brabantcloud specific elements.</documentation>
            </annotation>
            <complexContent>
                <extension base="rdf:AboutType">
                    <sequence>
                        <element ref="ore:isAggregatedBy" minOccurs="1" maxOccurs="1"/>
                       <xs:choice>
                           <element ref="nave:citName" minOccurs="0" maxOccurs="1"/>
                           <element ref="nave:citOldId" minOccurs="0" maxOccurs="1"/>
                           <element ref="nave:collection" minOccurs="1" maxOccurs="1"/>
                           <element ref="nave:collectionPart" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:color" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:colorHex" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:creatorBirthYear" minOccurs="0" maxOccurs="unbounded"/>
                           <element ref="nave:creatorDeathYear" minOccurs="0" maxOccurs="unbounded"/>
