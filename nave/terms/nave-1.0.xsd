                           <element ref="nave:year" minOccurs="0" maxOccurs="unbounded"/>
                       </xs:choice>
                    </sequence>
                </extension>
            </complexContent>
        </complexType>
    </xs:element>

    <xs:element name="DelvingResource">
        <complexType name="DelvingType">
            <annotation>
                <documentation>DelvingType contains Delving elements.</documentation>
            </annotation>
            <complexContent>
                <extension base="rdf:AboutType">
                    <sequence minOccurs="0" maxOccurs="unbounded">
                        <element ref="ore:isAggregatedBy" minOccurs="1" maxOccurs="1"/>
                        <choice>
                            <element ref="deepZoomUrl" minOccurs="0" maxOccurs="unbounded"/>
                            <element ref="fullText" minOccurs="0" maxOccurs="unbounded"/>
                            <element ref="fullTextObjectUrl" minOccurs="0" maxOccurs="unbounded"/>
                            <element ref="imageUrl" minOccurs="0" maxOccurs="unbounded"/>
                            <element ref="thumbnail" minOccurs="0" maxOccurs="unbounded"/>
                            <element ref="year" minOccurs="0" maxOccurs="unbounded"/>
                        </choice>
                    </sequence>
                </extension>
            </complexContent>
        </complexType>
    </xs:element>

    <xs:element name="DimconResource">
        <complexType>
            <annotation>
                <documentation>ResourceType contains Dimcon elements.</documentation>
            </annotation>
            <complexContent>
                <extension base="rdf:AboutType">
                    <sequence minOccurs="0" maxOccurs="unbounded">
