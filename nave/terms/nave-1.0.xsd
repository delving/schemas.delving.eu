                            <element ref="estateNr" minOccurs="0" maxOccurs="unbounded"/>
                            <element ref="imageUri" minOccurs="0" maxOccurs="unbounded"/>
                            <element ref="introduction" minOccurs="0" maxOccurs="unbounded"/>
                            <element ref="latLong" minOccurs="0" maxOccurs="unbounded"/>
                            <element ref="municipality" minOccurs="0" maxOccurs="unbounded"/>
                            <element ref="municipalityNr" minOccurs="0" maxOccurs="unbounded"/>
